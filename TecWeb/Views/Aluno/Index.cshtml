@model List<Aluno>
@{ 
    ViewBag.Title = "Aluno";
}

<br />
<div class="list-group">
    <a href="#" class="list-group-item active">
    Aluno
    </a>
    @foreach (var aluno in @Model) {
        // /Disciplina/Index?idAluno=@aluno.IdAluno&nomeAluno=@aluno.Nome">@aluno.Nome
        <a href="#" class="list-group-item"
           onclick="loadDoc('@Url.Action("index","Disciplina", new { IdAluno = @aluno.IdAluno, nomeAluno = @aluno.Nome })')">@aluno.Nome</a>
    }
</div>

<div id="demo">

</div>


